   "source": [
    "import cadquery as cq\n",
    "\n",
    "nut_diameter = 0.25\n",
    "nut_height = 0.25\n",
    "thread_diameter = 0.125\n",
    "\n",
    "# Create a hexagonal nut\n",
    "result = (\n",
    "    cq.Workplane(\"XY\")\n",
    "    .polygon(6, nut_diameter)\n",
    "    .extrude(nut_height)\n",
    "    .faces(\">Z\")\n",
    "    .workplane()\n",
    "    .hole(thread_diameter)\n",
    ")\n",
    "\n",
    "display(result)"
   ]
